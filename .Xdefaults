Xcursor.theme:          Vanilla-DMZ
URxvt*font:		xft:DejaVu Sans Mono:prgba=vbgr:pixelsize=12
urxvt-bg.font:		xft:FreeSerif:rgba=vbgr:pixelsize=12
URxvt*termName:         rxvt-unicode
URxvt*background:       #060606
URxvt*foreground:       #f6f6f6
URxvt*buffered: true
URxvt*perl-lib:         /usr/lib/urxvt/perl/
URxvt*perl-ext-common:  default,matcher
URxvt*matcher.button:   1
URxvt*urlLauncher:      /usr/bin/firefox
!URxvt*fading:           50
!URxvt*fadeColor:        #000000

URxvt*scrollBar:        false

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            true
Xft*hintingstyle:       full


URxvt.transparent:   true
URxvt.tintColor:     black
URxvt.shading:       5 
urxvt-bg.shading:    10 
 
#include ".xcolors/zenburn"
