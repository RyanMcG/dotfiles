#!/bin/sh
#
# ~/.xinitrc
#
# Executed by startx (run your window manager from here)
/usr/lib/systemd/systemd --user &
# exec gnome-session
# exec startkde
# exec startxfce4
# ...or the Window Manager of your choice
export DE="xfce"
export PATH=$HOME/bin:$PATH
export MPD_HOST=alarmpi
exec i3
# exec startxfce4
# exec openbox-session
